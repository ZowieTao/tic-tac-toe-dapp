{
  "name": "tic-tac-toe-dapp",
  "version": "1.0.0",
  "main": "index.js",
  "license": "AGPL-3.0",
  "description": "amazing tic tac toc dapp.",
  "sideEffects": false,
  "scripts": {
    "build": "next build",
    "dev": "next -p 1110",
    "prepare": "husky install",
